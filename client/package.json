{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.0",
    "react-select": "^4.3.0"
  },
  "proxy": "http://localhost:4000",
  "scripts": {
    "build-css": "postcss src/main.css -o src/index.css",
    "watch-css": "postcss src/main.css -o src/index.css -w",
    "start": "react-scripts start",
    "dev": "npm-run-all -p watch-css start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.3.2",
    "autoprefixer": "^10.2.5",
    "husky": "^3.0.3",
    "lint-staged": "^9.2.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.9",
    "postcss-cli": "^8.3.1",
    "prettier-eslint-cli": "^5.0.0",
    "tailwindcss": "^2.1.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,html}": [
      "prettier-eslint --write",
      "git add"
    ]
  }
}
